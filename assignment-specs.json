              ],
              "returns": "number",
              "tests": [
                { "input": [[1, 2, 2, 3, 2], 2], "expected": 3 },
                { "input": [["a", "b", "a"], "a"], "expected": 2 },
                { "input": [[1, 2, 3], 5], "expected": 0 }

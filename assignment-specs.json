            {
              "name": "greetUser",
              "description": "Return greeting message with name",
              "params": [{ "name": "name", "type": "string" }],
              "returns": "string",
              "tests": [
                { "input": ["Alice"], "expected": "Hello, Alice!" },
                { "input": ["Bob"], "expected": "Hello, Bob!" },
                { "input": [""], "expected": "Hello, !" }
              ]
            },
            {
              "name": "calculateArea",
              "description": "Calculate rectangle area",
              "params": [
                { "name": "width", "type": "number" },
                { "name": "height", "type": "number" }
              ],
              "returns": "number",
              "tests": [
                { "input": [5, 3], "expected": 15 },
                { "input": [10, 10], "expected": 100 },
                { "input": [0, 5], "expected": 0 }
              ]
            },
            {
              "name": "isAdult",
              "description": "Check if age >= 18",
              "params": [{ "name": "age", "type": "number" }],
              "returns": "boolean",
              "tests": [
                { "input": [18], "expected": true },
                { "input": [21], "expected": true },
                { "input": [17], "expected": false },
                { "input": [0], "expected": false }
              ]
            },
            {
              "name": "getFullName",
              "description": "Combine first and last name",
              "params": [
                { "name": "firstName", "type": "string" },
                { "name": "lastName", "type": "string" }
              ],
              "returns": "string",
              "tests": [
                { "input": ["John", "Doe"], "expected": "John Doe" },
                { "input": ["Jane", "Smith"], "expected": "Jane Smith" }
              ]
            },
            {
              "name": "convertToCelsius",
              "description": "Convert Fahrenheit to Celsius",
              "params": [{ "name": "fahrenheit", "type": "number" }],
              "returns": "number",
              "tests": [
                { "input": [32], "expected": 0 },
                { "input": [212], "expected": 100 },
                { "input": [98.6], "expected": 37, "tolerance": 0.1 }
              ]
            }
          ]
        }
      ]
    },

              ],
              "returns": "object",
              "tests": [
                { "input": [{ "a": 1 }, { "b": 2 }], "expected": { "a": 1, "b": 2 } },
                { "input": [{ "x": 1 }, { "x": 2 }], "expected": { "x": 2 } }
              ]
            },

                { "input": [{ "name": "Alice" }, "age"], "expected": false }
              ]
            },
            {
              "name": "countKeys",
              "description": "Count number of keys",
              "params": [{ "name": "obj", "type": "object" }],
              "returns": "number",
              "tests": [

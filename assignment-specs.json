                { "input": [[1, 2, 2, 3, 3, 3]], "expected": [1, 2, 3] },
                { "input": [["a", "b", "a"]], "expected": ["a", "b"] },
                { "input": [[]], "expected": [] }
              ]
            },
            {
              "name": "countOccurrences",

                { "input": [["a", "b"]], "expected": "b" },
                { "input": [[]], "expected": null }
              ]
            },
            {
              "name": "sumArray",
              "description": "Sum all numbers in array",
              "params": [{ "name": "arr", "type": "array" }],
              "returns": "number",
              "tests": [
                { "input": [[1, 2, 3, 4, 5]], "expected": 15 },
                { "input": [[10, 20]], "expected": 30 },
                { "input": [[]], "expected": 0 }
              ]
            },
            {
              "name": "findMax",
              "description": "Find maximum value",
              "params": [{ "name": "arr", "type": "array" }],
              "returns": "number|null",
              "tests": [
                { "input": [[1, 5, 3, 9, 2]], "expected": 9 },
                { "input": [[-5, -1, -10]], "expected": -1 },
                { "input": [[]], "expected": null }
              ]
            },
            {
              "name": "findMin",
